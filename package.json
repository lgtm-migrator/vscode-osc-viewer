{
  "name": "osc-viewer",
  "displayName": "osc-viewer",
  "description": "Viewer of the resource in the 3DS Outscale Cloud",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:osc-viewer.helloWorld",
    "onView:profile",
    "onCommand:osc-viewer.configure",
    "onCommand:osc.showResource",
    "onCommand:osc.deleteResource",
    "onCommand:osc.startVm",
    "onCommand:osc.stopVm"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "osc-viewer.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "profile.refreshEntry",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "profile.configure",
        "title": "Configure",
        "icon": "$(gear)"
      },
      {
        "command": "profile.addEntry",
        "title": "Add",
        "icon": "$(gist-new)"
      },
      {
        "command": "osc.showResource",
        "title": "Get"
      },
      {
        "command": "osc.deleteResource",
        "title": "Delete"
      },
      {
        "command": "osc.stopVm",
        "title": "Stop"
      },
      {
        "command": "osc.startVm",
        "title": "Start"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "package-explorer",
          "title": "3DS Outscale",
          "icon": "resources/outscale.png"
        }
      ]
    },
    "views": {
      "package-explorer": [
        {
          "id": "profile",
          "name": "Profile"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "profile.refreshEntry",
          "when": "view == profile",
          "group": "navigation"
        },
        {
          "command": "profile.configure",
          "when": "view == profile",
          "group": "navigation"
        },
        {
          "command": "profile.addEntry",
          "when": "view == profile",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "osc.deleteResource",
          "when": "view == profile && viewItem =~ /^resourcenode$|^vmresourcenode$/",
          "group": "0@3"
        },
        {
          "command": "osc.startVm",
          "when": "view == profile && viewItem == vmresourcenode",
          "group": "0@1"
        },
        {
          "command": "osc.stopVm",
          "when": "view == profile && viewItem == vmresourcenode",
          "group": "0@2"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/vscode": "^1.67.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "outscale-api": "github:outscale/osc-sdk-js",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "outscale-api": "github:outscale/osc-sdk-js"
  }
}